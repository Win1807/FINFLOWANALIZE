<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
	    id = "CPDialog"
		noDataText="{i18n>NoDataFound}"
		title="{i18n>CPDialogTitle}"
		liveChange="onHandleSearchForCP"
		search="onHandleSearchForCP"
		confirm="onHandleCloseForCP"
		close="onHandleCloseForCP"
		items="{
			path: '/FCLM_CFBA_CP_VHSet'
		}" >
		<StandardListItem
			title="{CashPoolName}"
			description="{CashPoolDesc}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>