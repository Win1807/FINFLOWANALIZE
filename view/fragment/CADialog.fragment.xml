<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>NoDataFound}"
		title="{i18n>CADialogTitle}"
		liveChange="onHandleSearchForCAL"
		search="onHandleSearchForCAL"
		confirm="onHandleCloseForUsrCal"
		close="onHandleCloseForUsrCal"
		items="{
			path: '/VL_SH_H_TFACD'
		}" >
		<StandardListItem
			title="{IDENT}"
			description="{LTEXT}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>